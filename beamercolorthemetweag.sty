\mode<presentation>

% Palette
\definecolor{black}{RGB}{0, 0, 0}
\definecolor{white}{RGB}{255, 255, 255}
\definecolor{goldenglow}{RGB}{254, 224, 128}
\definecolor{radicalred}{RGB}{255, 59, 90}
\definecolor{apricotpeach}{RGB}{254, 207, 177}
\definecolor{indigo}{RGB}{83, 115, 207}
\definecolor{daisybush}{RGB}{77, 34, 168}
\definecolor{oxfordblue}{RGB}{56, 69, 88}
\definecolor{emerald}{RGB}{85, 193, 113}
\definecolor{greenpea}{RGB}{0, 86, 65}
\definecolor{coral}{RGB}{252, 120, 81}
\definecolor{textgray}{gray}{0.25}
\definecolor{boxgray}{gray}{0.9}

% Settings
\setbeamercolor{palette primary}{bg=oxfordblue,fg=black}
\setbeamercolor{palette secondary}{bg=oxfordblue,fg=black}
\setbeamercolor{palette tertiary}{bg=oxfordblue,fg=black}
\setbeamercolor{palette quaternary}{bg=oxfordblue,fg=black}
\setbeamercolor{headline}{bg=oxfordblue, fg=black} % itemize, enumerate, etc
\setbeamercolor{normal text}{fg=textgray}

\setbeamercolor{frametitle}{bg=oxfordblue, fg=white}
\setbeamercolor{block title}{bg=boxgray, fg=black}
\setbeamercolor{title}{bg=oxfordblue, fg=white}
\setbeamercolor{date}{bg=oxfordblue, fg=white}
\setbeamercolor{author}{bg=oxfordblue, fg=white}

% Override palette coloring with secondary
\setbeamercolor{subsection in head/foot}{bg=oxfordblue,fg=white}

\mode
<all>
